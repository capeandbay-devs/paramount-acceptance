{
  "name": "capeandbay/paramount",
  "description": "Paramount Acceptance SDK implemented in thr Laravel Way",
  "license": "MIT",
  "keywords": [
    "paramount"
  ],
  "type": "library",
  "authors": [
    {
      "name": "",
      "email": ""
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/support": "^6.0|^7.0|^8.0",
    "ixudra/curl": "^6.21"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0|^5.0|^6.0",
    "phpunit/phpunit": "^8.4|^9.0"
  },
  "autoload": {
    "psr-4": {
      "CapeAndBay\\Paramount\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CapeAndBay\\Paramount\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "CapeAndBay\\Paramount\\ParamountServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
